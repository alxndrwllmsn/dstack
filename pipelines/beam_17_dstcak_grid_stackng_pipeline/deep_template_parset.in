Cdeconvolver.freqframe = bary
Cdeconvolver.imagetype = casa
Cdeconvolver.restore = true
Cdeconvolver.ncycles = 1
Cdeconvolver.memorybuffers = true
Cdeconvolver.Images.shape = [1024,1024]
Cdeconvolver.Images.direction = [22h42m21.852, -30.24.58.46, J2000]
Cdeconvolver.Images.cellsize = [6arcsec, 6arcsec]
Cdeconvolver.Images.writeAtMajorCycle = false
Cdeconvolver.Images.Names = ['image.deep']
#Cdeconvolver.Images.image.deep.direction = [22h42m21.852, -30.24.58.46, J2000]
Cdeconvolver.Images.image.deep.polarisation = ["I"]
Cdeconvolver.solver = Clean
Cdeconvolver.solver.Clean.algorithm = Basisfunction
Cdeconvolver.solver.Clean.scales = [0,6,15]
Cdeconvolver.solver.Clean.solutiontype = MAXBASE
Cdeconvolver.solver.Clean.niter = 5000
Cdeconvolver.solver.Clean.gain = 0.2
Cdeconvolver.solver.Clean.beam = [5, 5, 0.]
Cdeconvolver.threshold.minorcycle = [5%,5.1mJy,0.8mJy]
#Cdeconvolver.preconditioner.Names = ['Wiener']
Cdeconvolver.preconditioner.Names = [Wiener,GaussianTaper]
Cdeconvolver.preconditioner.GaussianTaper = [30arcsec, 30arcsec, 0deg] 
Cdeconvolver.preconditioner.GaussianTaper.isPsfSize  = true
Cdeconvolver.preconditioner.GaussianTaper.tolerance  = 0.005
Cdeconvolver.preconditioner.preservecf = true
Cdeconvolver.preconditioner.Wiener.robustness = 0.5
Cdeconvolver.restore.beam = fit
